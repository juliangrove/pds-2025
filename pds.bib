@article{aparicio_beware_2025,
	title = {Beware of referential garden paths! {The} dangerous allure of semantic parses that succeed locally but globally fail},
	volume = {4},
	issn = {2767-0279},
	url = {https://escholarship.org/uc/item/87v9q353},
	doi = {10.5070/G60111484},
	abstract = {A central endeavor in psycholinguistic research has been to determine the processing profile of syntactically ambiguous strings. Previous work investigating syntactic attachment ambiguities has shown that discarding a locally grammatically available, but globally failing, parse is costly. However, little is known about how comprehenders cope with semantic parsing ambiguities. Using the case study of scopally ambiguous definite descriptions such as the rabbit in the big hat, we examine whether comparable penalties arise for non-lexical semantic ambiguities. In a series of reference resolution tasks, we find dispreference for strings that are globally defined but fail to refer under alternative semantic parses, compared to strings where all readings successfully refer to the same individual. Crucially, this effect is only detectable when the alternative failing reading gives rise to a REFERENTIAL GARDEN PATH, where a dynamic constraint evaluation process temporarily settles on a unique referent before eventually failing. We conclude that failing alternative readings cause dispreference for a definite description, but only when the failing interpretation constitutes a red herring.},
	language = {en},
	number = {1},
	urldate = {2025-01-13},
	journal = {Glossa Psycholinguistics},
	author = {Aparicio, Helena and Levy, Roger and Coppock, Elizabeth},
