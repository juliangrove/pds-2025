	pages = {12002--12007},
	file = {Full Text PDF:/home/juliangrove/Documents/zotero/storage/3JWDTLVJ/Kao et al. - 2014 - Nonliteral understanding of number words.pdf:application/pdf},
}

@misc{liu_roberta_2019,
