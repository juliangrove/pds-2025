@phdthesis{mcnabb_syntax_2012,
	address = {United States -- Illinois},
	type = {Ph.{D}.},
	title = {The syntax and semantics of degree modification},
	copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
	url = {https://www.proquest.com/docview/1025746402/abstract/73135C3E2A44439DPQ/1},
	abstract = {Whether there is a one-to-one mapping between semantic categories and their syntactic counterparts has been the subject of extensive research in linguistic theory (e.g. Grimshaw 1979; Pesetsky 1982; Sadock 1991). Degree expressions are a case study for the investigation of the extent of isomorphism in the syntax-semantics mapping. Syntactically, they are characterized either as modifiers or specifiers (Bresnan, 1973; Jackendoff, 1977), as heads (Abney, 1987; Corver, 1997), or as a mixed class of heads and adjuncts (Neeleman et al., 2004). Likewise, semantic analyses of these expressions treat them as modifiers of context-sensitive properties of individuals (Klein 1980), as operators that take a gradable meaning and return a property of individuals meaning (Kennedy \& McNally, 2005a), or as a mixed class of semantic modifiers and operators, thereby mirroring their syntactic categories (Kennedy \& McNally, 2005b).
In order to determine whether the semantic categories ascribed to degree expressions correspond to their syntactic structure, I compare their syntactic and semantic distribution crosslinguistically. Data from Hebrew, Arabic, and Korean show that degree expressions are underlyingly heads that sometimes project as heads and sometimes as adjuncts as a result of morphological and syntactic operations. In addition, I show that the semantic combinatorics proposed in Kennedy \& McNally (2005b) are borne out modulo some language-specific morphosyntactic and semantic properties.
In the latter part of the dissertation, I develop an experimental paradigm consisting of picture description tasks that aims to adjudicate between two competing analyses of gradable predicates-whether they are context-sensitive properties (Klein, 1980) or relations between individuals and degrees (Cresswell, 1976; von Stechow, 1984; Kennedy, 1997). Another goal of these experiments is to determine what the semantic properties of the intensifier very are as a representative of a class of degree modifiers that manipulate contextual parameters associated with the gradable predicate they modify. Results from two studies lend support to the degree analysis and show that very is indeed sensitive to contextual parameters, some of which are involved in the semantics of degree modifiers and some of which are not. The crosslinguistic picture that emerges includes partial isomorphism in the syntax-semantics mapping of degree expressions within predictable parameters.},
	language = {English},
	urldate = {2024-07-03},
	school = {The University of Chicago},
	author = {McNabb, Yaron},
	year = {2012},
	note = {ISBN: 9781267438010},
	keywords = {Semantics, Language, Psycholinguistics, literature and linguistics, Syntax, Adjectives, Arabic, Degree expressions, Hebrew, Korean, Morphology, Philosophy, Psychology, religion and theology},
	file = {Full Text PDF:/home/juliangrove/Documents/zotero/storage/GUKWPSTE/McNabb - The syntax and semantics of degree modification.pdf:application/pdf},
}

